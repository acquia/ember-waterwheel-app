<h2>All Articles</h2>

{{#link-to 'articles.new'}}New Article{{/link-to}}

{{outlet}}

{{#each model as |article|}}
    <h3><a href="/article/{{article.uuid}}">{{article.title}}</a></h3>
    <ul>
        <li>NID: {{article.nid}}</li>
        <li>UUID: {{article.uuid}}</li>
        <li>Created: {{article.createdDate}}</li>
    </ul>
{{/each}}
